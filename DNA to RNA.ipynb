   "source": [
    "dna = input()\n",
    "new = \"\"\n",
    " \n",
    "for i in dna:\n",
    "    if i not in 'ATGC':\n",
    "        new = \"Invalid Input\"\n",
    "        break\n",
    "    if i == 'A':\n",
    "        new += 'U'\n",
    "    elif i == 'C':\n",
    "        new += 'G'\n",
    "    elif i == 'T':\n",
    "        new += 'A'\n",
    "    else:\n",
    "        new += 'C'\n",
    " \n",
    "print(new)"
